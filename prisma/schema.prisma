
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  username String @unique
  password String
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt
  @@map("users")
}
